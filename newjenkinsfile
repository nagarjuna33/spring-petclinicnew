pipeline {
    agent { 
	node { label 'MVN3' }
        }
		}
    stages {
        stage('vcs') {
            steps {
             git url: 'https://github.com/nagarjunaduggireddy/spring-petclinicmain.git', 
             branch: 'main'
            }
			}
   stage ('Exec Maven') {
            steps {
                rtMavenRun (
                    pom: 'pom.xml',
                    goals: 'clean install',
                      )
            }
        }
	stage ('Artifactory configuration') {
            steps {
                
           rtMavenDeployer (
           id: "MAVEN_DEPLOYER",
           serverId: "ARJUN_JFROG",
           releaseRepo: 'default-libs-release-local',
           snapshotRepo: 'default-libs-snapshot-local'
                )

            }
        }
        stage('Build the Code') {
            steps {
                withSonarQubeEnv('SONAR_SELF_HOSTED') {
                    sh script: 'mvn clean package sonar:sonar'
                }
            }
        }
    }
}
